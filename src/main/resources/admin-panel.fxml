<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" styleClass="anchors" stylesheets="@style.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.easvtickets.GUI.Controller.AdminPanelController">
   <children>
      <Button fx:id="deleteButtonAdmin" layoutX="25.0" layoutY="203.0" mnemonicParsing="false" onAction="#onAdminDeleteButtonPressed" prefHeight="25.0" prefWidth="144.0" styleClass="button-login" stylesheets="@style.css" text="Delete Event/User" />
      <Label layoutX="200.0" layoutY="32.0" text="Currently  Selected Entity">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="saveButtonAdmin" layoutX="438.0" layoutY="167.0" mnemonicParsing="false" prefHeight="60.0" prefWidth="133.0" styleClass="button-login" stylesheets="@style.css" text="Save" />
      <Button fx:id="createUserButton" layoutX="25.0" layoutY="172.0" mnemonicParsing="false" onAction="#onCreateUserButtonPressed" prefHeight="18.0" prefWidth="144.0" styleClass="button-login" stylesheets="@style.css" text="Create User" />
      <TextArea layoutX="200.0" layoutY="72.0" prefHeight="308.0" prefWidth="200.0" styleClass="tables" stylesheets="@style.css" />
      <ImageView fitHeight="85.0" fitWidth="126.0" layoutX="34.0" layoutY="72.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@images/easv.jpg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
